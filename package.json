{
  "name": "yubico-js",
  "version": "0.0.12",
  "description": "A Yubico OTP Client",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watchAll",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "patch": "npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohitkyadav/yubico-js.git"
  },
  "keywords": [
    "Yubico",
    "Yubikey"
  ],
  "author": "Mohit Kumar Yadav",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohitkyadav/yubico-js/issues"
  },
  "homepage": "https://github.com/mohitkyadav/yubico-js#readme",
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "dotenv": "^16.0.1",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
